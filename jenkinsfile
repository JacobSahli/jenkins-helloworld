pipeline {
  agent any
  stages {
    stage('clone') {
       steps{
          sh "rm -rf *"
          sh "https://github.com/JacobSahli/jenkins-helloworld.git"
       }
    }
    stage('build') {
       steps {
          sh "cd jenkins-helloworld/ && javac Main.java"
       }
    }
    stage('run') {
       steps {
        sh "cd jenkins-helloworld/ && java Main"
       }
    }
  }
}
